# Snowflake Database Configuration
# Copy this file to secrets.toml and fill in your actual credentials

[snowflake]
account = "your-account.region.aws"
user = "your-username"
password = "your-password"
warehouse = "COMPUTE_WH"
database = "CHILD_NUTRITION_DB"
schema = "NUTRITION_DATA"
role = "ACCOUNTADMIN"

# OpenAI Configuration (for AI features)
[openai]
api_key = "your-openai-api-key"

# Application Configuration
[app]
title = "Child Nutrition Dashboard"
description = "Comprehensive nutrition analysis and monitoring dashboard"